<table width="50%">

    <tr>
        <th>firstName </th>
        <th>lastName </th>
        <th>email </th>
        <th>phone </th>
        <th>contactCode </th>

        <th>websiteURL </th>
        <th>resume </th>

    </tr>

    <tbody>
        <% applyModel.forEach(function(data) { %>
            <tr>
                <td>
                    <%= data.firstName %>
                </td>
                <td>
                    <%= data.lastName %>
                </td>
                <td>
                    <%= data.email %>
                </td>
                <td>
                    <%= data.phone %>
                </td>
                <td>
                    <%= data.contactCode %>
                </td> 
                <td>
                    <%= data.websiteURL %>
                </td>
                <td>
                    <%= data.resume %>
                </td>
            </tr>
            <% }); %>
    </tbody>

</table>